{
  "name": "@wikia/react-design-system",
  "version": "2.2.4",
  "description": "Wikia's reusable React parts",
  "repository": "git@github.com:Wikia/react-design-system.git",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn lib:build && yarn styleguide:build && yarn package:build && yarn readme:build",
    "clean": "node ./scripts/clean.js",
    "coveralls": "yarn test --runInBand && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "concurrently --names \"SERVER,TESTS\" --prefix \"{time}|{name}\" \"yarn styleguide:watch\" \"yarn test:watch\"",
    "lib:build": "rollup -c config/rollup.config.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint \"components/*.css\"",
    "lint:fix": "stylelint \"components/*.css\" --fix",
    "lint:js": "eslint --cache .",
    "readme:build": "node ./scripts/build-readme.js",
    "package:build": "node ./scripts/build-package.json.js",
    "bump:version": "node ./scripts/bump-version.js",
    "create:tag": "node ./scripts/create-tag.js",
    "release": "yarn release:patch",
    "release:major": "git checkout master && git pull && yarn bump:version major && yarn build && git add .. && yarn create:tag && git push --follow-tags",
    "release:minor": "git checkout master && git pull && yarn bump:version minor && yarn build && git add .. && yarn create:tag && git push --follow-tags",
    "release:patch": "git checkout master && git pull && yarn bump:version patch && yarn build && git add .. && yarn create:tag && git push --follow-tags",
    "styleguide:build": "styleguidist build --config ./config/styleguide.config.js",
    "styleguide:watch": "styleguidist server --config ./config/styleguide.config.js",
    "ci": "yarn lint && yarn test --runInBand",
    "test": "jest --config ./config/jest.config.js",
    "test:update": "yarn test -u",
    "test:watch": "yarn test --watchAll --notify"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.3",
    "classnames": "^2.2.6",
    "concurrently": "^4.0.1",
    "coveralls": "^3.0.2",
    "date-fns": "^1.29.0",
    "design-system": "git://github.com/Wikia/design-system.git#14.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "glob": "^7.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lodash.uniqueid": "^4.0.1",
    "node-sass": "^4.9.3",
    "postcss-modules": "^1.3.2",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-styleguidist": "^7.3.8",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-string": "^2.0.2",
    "sass-loader": "^7.1.0",
    "semver": "^5.5.1",
    "sinon": "^6.3.4",
    "stylelint": "^9.5.0",
    "stylelint-config-fandom": "git://github.com/Wikia/stylelint-config-fandom.git#0.0.1",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  }
}
