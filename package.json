{
  "name": "@wikia/react-design-system",
  "version": "1.1.1",
  "description": "Reusable React components based on Wikia's Design System",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "style": "dist/index.css",
  "mainIcons": "dist/icons.cjs.js",
  "moduleIcons": "dist/icons.esm.js",
  "styleIcons": "dist/icons.css",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:Wikia/react-design-system.git",
  "license": "Apache-2.0",
  "dependencies": {
    "design-system": "git://github.com/Wikia/design-system.git#12.0.0",
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "styleguide:watch": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "lib:build": "yarn clean && rollup -c",
    "build": "yarn lib:build && yarn styleguide:build",
    "coverage": "jest --coverage",
    "coveralls": "yarn coverage --runInBand && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "concurrently --names \"SERVER,TESTS\" --prefix \"{time}|{name}\" \"yarn styleguide:watch\" \"yarn test:watch\"",
    "lint:css": "stylelint \"components/*.css\"",
    "lint:fix": "stylelint \"components/*.css\" --fix",
    "lint:js": "eslint --cache .",
    "lint": "yarn lint:js && yarn lint:css",
    "release:major": "git checkout master && git pull && yarn build && git add . && yarn version --new-version major && git push --follow-tags",
    "release:minor": "git checkout master && git pull && yarn build && git add . && yarn version --new-version minor && git push --follow-tags",
    "release:patch": "git checkout master && git pull && yarn build && git add . && yarn version --new-version patch && git push --follow-tags",
    "release": "yarn release:patch",
    "test:run": "jest",
    "test:watch": "jest --watchAll --notify",
    "test": "yarn lint && yarn coverage"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "node-sass": "^4.5.3",
    "postcss-modules": "^1.3.2",
    "raw-loader": "^0.5.1",
    "react-styleguidist": "^7.2.1",
    "react-test-renderer": "^16.2.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-string": "^2.0.2",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "stylelint": "^9.1.3",
    "stylelint-config-fandom": "git://github.com/Wikia/stylelint-config-fandom.git#0.0.1",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
